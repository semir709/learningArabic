<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.15.4/css/all.css" integrity="sha384-DyZ88mC6Up2uqS4h/KRgHuoeGwBcD4Ng9SiP4dIRy0EXTlnuz47vAwmeGwVChigm" crossorigin="anonymous">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/all_words_admin.css">
    <link rel="stylesheet" href="/css/navbar.css">
    <title>Hello, world!</title>
  </head>
  <body>
    
    <%-include("partials/navbar.ejs")%>


    <div class="container ss_container_holder">
      <div class="row">
        <div class="col-12">
          <div class="d-flex align-items-center ss_line ss_header_holder  ">
            <div class="input-group ss_input_holder ">
              <input type="text" id="category" class="form-control" placeholder="Select category" oninput="resizeInput(event)" >
              <button class="btn dropdown-toggle" type="button" data-bs-toggle="dropdown" ></button>
              <ul class="dropdown-menu ">
                <% all_category.forEach(d => { %>
                  <li><a class="dropdown-item" onclick="selectVal_category(event)"><%= d._name%></a></li>   
                <% }) %>
              </ul>
            </div>

            <p class="ms-auto mb-0 ss_p_del_content" id="ss_del_btn">Delete category</p>
        </div>
        </div>
      </div>

      <div class="row">
          <div class="col-12" id="ss_col_data">
              
          </div>
      </div>
    </div>

    <div class="container-fluid">
      <div class="row">

      </div>
    </div>
    <script src="/js/jquery/jquery.js"></script>
    <script src="/js/admin_all.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
  </body>
</html>